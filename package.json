{
  "name": "boilerplate-api",
  "version": "1.0.0",
  "description": "Boilerplate with Express",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "local": "nodemon index.js",
    "dev": "node index.js",
    "prod": "node index.js",
    "start": "node index.js",
    "migrate": "npx sequelize db:migrate",
    "migrate:undo": "npx sequelize db:migrate:undo:all",
    "seed": "npx sequelize db:migrate && npx sequelize db:seed:all",
    "reset-database": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate && npx sequelize db:seed:all",
    "lint": "eslint --fix src --ext .js  "
  },
